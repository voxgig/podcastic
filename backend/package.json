{
  "name": "@voxgig/podmind-backend",
  "version": "0.6.2",
  "description": "Podmind Backend",
  "main": "index.js",
  "scripts": {
    "xtest": "jest --coverage",
    "xtest-some": "jest -t",
    "xtest-watch": "jest --coverage --watchAll",
    "xtest-view": "open coverage/lcov-report/index.html",
    "test": "node --enable-source-maps --test dist-test/unit",
    "integ": "node --enable-source-maps --test dist-test/integ",
    "model": "voxgig-model model/model.jsonic",
    "model-build": "voxgig-model model/model.jsonic --once",
    "watch": "tsc --build src test -w",
    "build": "npm run model-build && tsc --build src && tsc --build test",
    "clean": "rm -rf dist dist-test node_modules yarn.lock package-lock.json",
    "reset": "npm run clean && npm install && npm run build && npm test",
    "prep-dev": "npm link @voxgig/podmind-common",
    "prep-vxg": "npm link @voxgig/podmind-common @voxgig/model @voxgig/build @voxgig/system @seneca/bedrock-chat",
    "local": "node dist/env/local/local.js --seneca.log.warn",
    "local-debug": "node dist/env/local/local.js --seneca.log.debug",
    "repl": "seneca-repl localhost:50402",
    "repl-dev": "seneca-repl aws://lambda/vxgpdm01-backend01-dev-monitor?region=us-east-1",
    "repl-dev-ingest": "seneca-repl aws://lambda/vxgpdm01-backend01-dev-ingest?region=us-east-1",
    "repl-dev-store": "seneca-repl aws://lambda/vxgpdm01-backend01-dev-store?region=us-east-1",
    "repl-dev-audio": "seneca-repl aws://lambda/vxgpdm01-backend01-dev-audio?region=us-east-1",
    "check-aws-profile": "node -e \"process.env.AWS_PROFILE.includes('podmind') || (console.log('BAD AWS_PROFILE'), process.exit(1))\"",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: pdm-be-v$REPO_VERSION && git add -u . && git commit -m pdm-be-v$REPO_VERSION && git push && git tag pdm-be-v$REPO_VERSION && git push --tags",
    "deploy-dev": "npm run reset && npm run deploy-dev-quick",
    "deploy-dev-quick": "npm run deploy-quick && serverless deploy --stage dev",
    "deploy-quick": "npm run check-aws-profile && echo AWS_PROFILE=$AWS_PROFILE && npm run build && npm run test && npm run repo-tag"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.535.0",
    "@deepgram/sdk": "^3.2.0",
    "@opensearch-project/opensearch": "^2.6.0",
    "@seneca/bedrock-chat": "^0.0.3",
    "@seneca/capture": "^0.5.0",
    "@seneca/config": "^0.4.0",
    "@seneca/dynamo-store": "6.1.0",
    "@seneca/entity-util": "2.0.0",
    "@seneca/env": "0.3.0",
    "@seneca/gateway": "1.2.0",
    "@seneca/gateway-auth": "1.0.2",
    "@seneca/gateway-lambda": "1.0.0",
    "@seneca/localque-transport": "^0.1.0",
    "@seneca/opensearch-store": "^0.0.1",
    "@seneca/owner": "^6.1.0",
    "@seneca/provider": "^3.0.1",
    "@seneca/reload": "0.4.1",
    "@seneca/repl": "8.1.1",
    "@seneca/s3-store": "^2.3.0",
    "@seneca/sqs-transport": "^0.5.0",
    "@seneca/telemetry": "^0.0.3",
    "@seneca/user": "6.3.0",
    "@seneca/webflow-provider": "^0.2.0",
    "@voxgig/model": "1.4.0",
    "@voxgig/podmind-common": "^0.0.7",
    "@voxgig/system": "0.9.0",
    "argparse": "2.0.1",
    "axios": "^1.6.8",
    "llm-chunk": "^0.0.1",
    "rate-limiter-flexible": "^5.0.0",
    "rss-parser": "^3.13.0",
    "seneca": "3.35.2",
    "seneca-entity": "25.1.3",
    "seneca-promisify": "3.7.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.535.0",
    "@aws-sdk/client-dynamodb": "^3.535.0",
    "@aws-sdk/client-lambda": "^3.535.0",
    "@aws-sdk/client-s3": "^3.535.0",
    "@aws-sdk/s3-request-presigner": "^3.535.0",
    "@aws-sdk/util-dynamodb": "^3.535.0",
    "@hapi/code": "^9.0.3",
    "@seneca/gateway-express": "0.12.0",
    "@tsconfig/node16": "16.1.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/uuid": "^9.0.8",
    "@voxgig/build": "2.3.2",
    "cookie-parser": "1.4.6",
    "es-jest": "2.1.0",
    "esbuild": "0.20.2",
    "express": "4.18.3",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "serverless": "^3.38.0",
    "tsx": "^4.7.1",
    "typescript": "5.4.2"
  }
}
